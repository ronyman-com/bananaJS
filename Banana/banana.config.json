{
  "entry": "./src/main.jsx",
  "outDir": "dist",
  "esbuild": {
    "minify": true,
    "sourcemap": true,
    "jsxImportSource": "react",
    "target": "es2020",
    "jsx": "automatic",
    "loader": {
      ".js": "jsx",
      ".jsx": "jsx"
    }
  },
  "alias": {
    "@": "./src",
    "~": "./src"
  }
}